    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JsnumeroSerie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jsprecio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JsNombreProducto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JsDisponible)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JsFechaAgregado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.JsProductoID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.JsProductoID">Details</a> |
