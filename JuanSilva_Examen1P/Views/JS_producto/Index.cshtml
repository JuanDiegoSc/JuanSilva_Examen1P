@model IEnumerable<JuanSilva_Examen1P.Models.JS_producto>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JsnumeroSerie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jsprecio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JsNombreProducto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JsDisponible)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JsFechaAgregado)
            </th>
            <th></th>
        </tr>
